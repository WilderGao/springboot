<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qg.springboot.mysql.dao.StudentDao">
    <select id="selectAllStudent" resultType="com.qg.springboot.model.Student">
        SELECT id AS 'id',
            student_id AS 'studentId',
            name AS 'name'
            FROM student
    </select>
</mapper>